<h2>Passages</h2>


<div id="categories-list">
    <% _.each(categories, function(category, id){%>
    <%if(!_.isEmpty(category.text)){%>
    <button class="select-button" data-id="<%=id%>" style="background-color: <%=category.backgroundColor%>; color: <%=category.color%>"><%=category.text%></button>
    <%}%>
    <%})%>
</div>
